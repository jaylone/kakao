{
  "name": "kakao",
  "version": "0.3.0",
  "description": "Kakao is an ORM and RESTful nodejs web framework based Koa2 like Ruby on Rails.",
  "keywords": [
    "ORM",
    "bookshelf",
    "example",
    "koa",
    "node on rails"
  ],
  "license": "MIT",
  "author": "zhongzhi <zhong.zhi@163.com>",
  "main": "index.js",
  "scripts": {
    "changelog": "github_changelog_generator --bug-labels --enhancement-labels --future-release=$npm_config_release --header-label='# Changelog'",
    "eslint": "eslint **/*.js",
    "migrate:latest": "knex migrate:latest && knex seed:run",
    "migrate:rollback": "knex migrate:rollback",
    "production": "NODE_ENV=production node_modules/.bin/pm2 index.js",
    "serve": "DEBUG=knex:query NODE_ENV=development babel-node server.js",
    "start": "DEBUG=knex:query NODE_ENV=development supervisor --ignore node_modules index.js",
    "start2": "knex migrate:latest && knex seed:run && NODE_ENV=development PORT=3000 npm run watch:server"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "bookshelf": "^0.10.2",
    "bookshelf-api": "^1.7.0",
    "bookshelf-cascade-delete": "^2.0.0",
    "bookshelf-modelbase": "^2.10.2",
    "bookshelf-uuid": "^1.0.0",
    "file-stream-rotator": "0.0.7",
    "get-prime-factors": "^1.0.0",
    "import-dir": "0.0.1",
    "joi": "^10.0.1",
    "knex": "^0.12.6",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-compose": "^2.5.1",
    "koa-json": "^2.0.0",
    "koa-morgan": "^1.0.1",
    "koa-qs": "^2.0.0",
    "koa-router": "^7.0.1",
    "lodash": "^4.17.2",
    "mkdirp": "^0.5.1",
    "mysql": "^2.12.0",
    "object-assign": "^4.1.0",
    "packing-glob": "^1.0.1",
    "pluralize": "^3.0.0",
    "uuid": "^3.0.1",
    "winston": "^2.3.0",
    "winston-daily-rotate-file": "^1.4.0"
  },
  "devDependencies": {
    "casual": "^1.5.8",
    "eslint": "^3.11.0",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-import": "^2.2.0",
    "supervisor": "^0.12.0"
  },
  "engines": {
    "iojs": ">= 1.0.0",
    "node": ">= 4.0.0"
  },
  "pre-commit": [
    "eslint"
  ]
}
